  io.smallrye.reactive.streams.operators; version="0.4.1", \
  io.smallrye.reactive.streams.spi; version="0.4.1", \
  io.smallrye.reactive.streams.stages; version="0.4.1", \
  io.smallrye.reactive.streams.utils; version="0.4.1", \

Include-Resource: \
  @${repo;com.ibm.ws.io.smallrye.reactive:smallrye-reactive-streams-operators;0.4.1}

WS-TraceGroup: REACTIVESTREAMS

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/third-party

